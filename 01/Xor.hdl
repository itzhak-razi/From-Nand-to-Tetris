/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {

    IN a, b;
    OUT out;

    PARTS:
    // Put your code here.
    Not(in = a, out = nota);
    Not(in = b, out = notb);
    And(a = a, b = notb, out = o1);
    And(a = nota, b = b, out = o2);
    Or(a = o1, b = o2, out = out);
}
